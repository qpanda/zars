	<tag>
		<name>switch</name>
		<tag-class>org.primefaces.touch.component.inputswitch.InputSwitchTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>id</name>
			<required>false</required>
			<description><![CDATA[Id of the component]]></description>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>rendered</name>
			<required>false</required>
			<description><![CDATA[Boolean value to specify the visibility of the component]]></description>
			<deferred-value>
				<type>boolean</type>
			</deferred-value>
		</attribute>
		<attribute>
			<name>binding</name>
			<required>false</required>
			<description><![CDATA[An EL expression that maps to a server-side component instance
		in a backing bean]]></description>
			<deferred-value>
				<type>javax.faces.component.UIComponent</type>
			</deferred-value>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<description><![CDATA[Value of the component that can be either an EL expression or a
		literal text]]></description>
			<deferred-value>
				<type>java.lang.Object</type>
			</deferred-value>
		</attribute>
		<attribute>
			<name>converter</name>
			<required>false</required>
			<description><![CDATA[An EL expression or a literal text that defines a converter for
		the component. When it's an EL expression, it's resolved to a
		converter instance. In case it's a static text, it must refer to
		a converter id]]></description>
			<deferred-value>
				<type>java.faces.convert.Converter</type>
			</deferred-value>
		</attribute>
		<attribute>
			<name>immediate</name>
			<required>false</required>
			<description><![CDATA[Boolean value that specifies the lifecycle phase the
		valueChangeEvents should be processed, when true the events will
		be fired at "apply request values", if immediate is set to
		false, valueChangeEvents are fired in "process validations"
		phase. Default is false]]></description>
			<deferred-value>
				<type>boolean</type>
			</deferred-value>
		</attribute>
		<attribute>
			<name>required</name>
			<required>false</required>
			<description><![CDATA[Boolean value that makes the component required, default value
		is false]]></description>
			<deferred-value>
				<type>boolean</type>
			</deferred-value>
		</attribute>
		<attribute>
			<name>validator</name>
			<required>false</required>
			<description><![CDATA[A method binding EL expression that refers to a method for
		validating the input]]></description>
			<deferred-value>
				<type>javax.faces.validator.Validator</type>
			</deferred-value>
		</attribute>
		<attribute>
			<name>valueChangeListener</name>
			<required>false</required>
			<description><![CDATA[A method binding EL expression that refers to a method for
		handling a value change event]]></description>
			<deferred-method>
				<method-signature>void valueChange(javax.faces.event.ValueChangeEvent)</method-signature>
			</deferred-method>
		</attribute>
		<attribute>
			<name>requiredMessage</name>
			<required>false</required>
			<description><![CDATA[Message to be displayed when required field validation fails]]></description>
			<deferred-value>
				<type>java.lang.String</type>
			</deferred-value>
		</attribute>
		<attribute>
			<name>converterMessage</name>
			<required>false</required>
			<description><![CDATA[Message to be displayed when conversion fails]]></description>
			<deferred-value>
				<type>java.lang.String</type>
			</deferred-value>
		</attribute>
		<attribute>
			<name>validatorMessage</name>
			<required>false</required>
			<description><![CDATA[Message to be displayed when validation fails]]></description>
			<deferred-value>
				<type>java.lang.String</type>
			</deferred-value>
		</attribute>
	</tag>
